{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${fileDirname}",
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    }
  ]
}
